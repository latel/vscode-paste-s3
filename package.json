{
  "name": "paste-and-upload",
  "version": "0.3.1",
  "publisher": "duanyll",
  "engines": {
    "vscode": "^1.97.0"
  },
  "license": "MIT",
  "displayName": "%displayName%",
  "description": "%description%",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Duanyll/paste-and-upload"
  },
  "keywords": [
    "paste",
    "upload",
    "image",
    "picture",
    "markdown"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "icon": "assets/icon.png",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "The extension does not read S3 credentials from untrusted workspaces.",
      "restrictedConfigurations": [
        "paste-and-upload.s3"
      ]
    }
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "paste-and-upload.testS3Connection",
        "title": "%command.testS3Connection.title%"
      },
      {
        "command": "paste-and-upload.undoRecentUpload",
        "title": "%command.undoRecentUpload.title%"
      }
    ],
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "paste-and-upload.s3.region": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.region.description%"
        },
        "paste-and-upload.s3.endpoint": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.endpoint.description%"
        },
        "paste-and-upload.s3.accessKeyId": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.accessKeyId.description%"
        },
        "paste-and-upload.s3.secretAccessKey": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.secretAccessKey.description%"
        },
        "paste-and-upload.s3.bucket": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.bucket.description%"
        },
        "paste-and-upload.s3.prefix": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.s3.prefix.description%"
        },
        "paste-and-upload.s3.publicUrlBase": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.s3.publicUrlBase.description%"
        },
        "paste-and-upload.s3.omitExtension": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.s3.omitExtension.description%"
        },
        "paste-and-upload.s3.skipExisting": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.s3.skipExisting.description%"
        },
        "paste-and-upload.s3.forcePathStyle": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.s3.forcePathStyle.description%"
        },
        "paste-and-upload.s3.clientOptions": {
          "type": "string",
          "default": "{}",
          "markdownDescription": "%configuration.s3.clientOptions.description%"
        },
        "paste-and-upload.workspace.path": {
          "type": "string",
          "default": "images",
          "description": "%configuration.workspace.path.description%"
        },
        "paste-and-upload.workspace.linkBase": {
          "type": "string",
          "default": "images/",
          "description": "%configuration.workspace.linkBase.description%"
        },
        "paste-and-upload.enabled": {
          "type": "boolean",
          "default": true,
          "scope": "language-overridable",
          "description": "%configuration.enabled.description%",
          "order": 0
        },
        "paste-and-upload.uploadDestination": {
          "type": "string",
          "default": "s3",
          "enum": [
            "s3",
            "workspace"
          ],
          "enumDescriptions": [
            "%configuration.uploadDestination.enum.s3%",
            "%configuration.uploadDestination.enum.workspace%"
          ],
          "scope": "language-overridable",
          "description": "%configuration.uploadDestination.description%",
          "order": 1
        },
        "paste-and-upload.fileSizeLimit": {
          "type": "integer",
          "default": 10000000,
          "scope": "language-overridable",
          "description": "%configuration.fileSizeLimit.description%"
        },
        "paste-and-upload.allowMultipleFiles": {
          "type": "string",
          "default": "prompt",
          "enum": [
            "prompt",
            "allow",
            "deny"
          ],
          "scope": "language-overridable",
          "description": "%configuration.allowMultipleFiles.description%"
        },
        "paste-and-upload.mimeTypeDetectionMethod": {
          "type": "string",
          "default": "content",
          "enum": [
            "content",
            "extension",
            "none"
          ],
          "enumItemLabels": [
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.content%",
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.extension%",
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.none%"
          ],
          "markdownEnumDescriptions": [
            "%configuration.mimeTypeDetectionMethod.enum.content%",
            "%configuration.mimeTypeDetectionMethod.enum.extension%",
            "%configuration.mimeTypeDetectionMethod.enum.none%"
          ],
          "scope": "language-overridable",
          "description": "%configuration.mimeTypeDetectionMethod.description%"
        },
        "paste-and-upload.keepOriginalFilename": {
          "type": "boolean",
          "default": false,
          "scope": "language-overridable",
          "markdownDescription": "%configuration.keepOriginalFilename.description%"
        },
        "paste-and-upload.fileNamingMethod": {
          "type": "string",
          "default": "md5Short",
          "enum": [
            "md5",
            "md5Short",
            "uuid",
            "nanoid",
            "unixTimestamp",
            "readableTimestamp",
            "prompt"
          ],
          "enumItemLabels": [
            "%configuration.fileNamingMethod.enumItemLabel.md5%",
            "%configuration.fileNamingMethod.enumItemLabel.md5Short%",
            "%configuration.fileNamingMethod.enumItemLabel.uuid%",
            "%configuration.fileNamingMethod.enumItemLabel.nanoid%",
            "%configuration.fileNamingMethod.enumItemLabel.unixTimestamp%",
            "%configuration.fileNamingMethod.enumItemLabel.readableTimestamp%",
            "%configuration.fileNamingMethod.enumItemLabel.prompt%"
          ],
          "enumDescriptions": [
            "%configuration.fileNamingMethod.enum.md5%",
            "%configuration.fileNamingMethod.enum.md5Short%",
            "%configuration.fileNamingMethod.enum.uuid%",
            "%configuration.fileNamingMethod.enum.nanoid%",
            "%configuration.fileNamingMethod.enum.unixTimestamp%",
            "%configuration.fileNamingMethod.enum.readableTimestamp%",
            "%configuration.fileNamingMethod.enum.prompt%"
          ],
          "scope": "language-overridable",
          "markdownDescription": "%configuration.fileNamingMethod.description%",
          "order": 2
        },
        "paste-and-upload.defaultSnippet": {
          "type": "string",
          "default": "${url}",
          "scope": "language-overridable",
          "markdownDescription": "%configuration.defaultSnippet.description%",
          "order": 3
        },
        "paste-and-upload.imageSnippet": {
          "type": "string",
          "default": "${url}",
          "scope": "language-overridable",
          "markdownDescription": "%configuration.imageSnippet.description%",
          "order": 4
        },
        "paste-and-upload.undoLimit": {
          "type": "integer",
          "default": 10,
          "markdownDescription": "%configuration.undoLimit.description%"
        },
        "paste-and-upload.mimeTypeFilter": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.mimeTypeFilter.description%",
          "order": 5
        },
        "paste-and-upload.ignoreWorkspaceFiles": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%configuration.ignoreWorkspaceFiles.description%",
          "order": 6
        },
        "paste-and-upload.retrieveOriginalImage": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%configuration.retrieveOriginalImage.description%",
          "order": 7
        }
      }
    },
    "configurationDefaults": {
      "[markdown]": {
        "paste-and-upload.defaultSnippet": "[${1:$TM_SELECTED_TEXT}](${url})",
        "paste-and-upload.imageSnippet": "![${1:$TM_SELECTED_TEXT}](${url})"
      },
      "[tex][latex]": {
        "paste-and-upload.uploadDestination": "workspace",
        "paste-and-upload.fileNamingMethod": "prompt",
        "paste-and-upload.imageSnippet": "\\includegraphics{${url}}"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.738.0",
    "@aws-sdk/lib-storage": "^3.738.0",
    "@babel/core": "^7.26.7",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.7",
    "@changesets/cli": "^2.29.8",
    "@types/lodash": "^4.17.14",
    "@types/mime-types": "^2.1.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "axios": "^1.10.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.16.0",
    "file-type": "^20.0.0",
    "filesize": "^10.1.6",
    "lodash": "^4.17.21",
    "md5.js": "^1.3.5",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.9",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.2",
    "uuid": "^11.0.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {}
}
