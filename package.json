{
  "name": "paste-s3",
  "version": "0.3.1",
  "publisher": "okwang",
  "engines": {
    "vscode": "^1.97.0"
  },
  "l10n": "./l10n",
  "license": "MIT",
  "displayName": "%displayName%",
  "description": "%description%",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/latel/vscode-paste-s3"
  },
  "keywords": [
    "paste",
    "upload",
    "image",
    "picture",
    "markdown"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "icon": "assets/icon.png",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "The extension does not read S3 credentials from untrusted workspaces.",
      "restrictedConfigurations": [
        "pasteS3.s3"
      ]
    }
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "paste-s3.testS3Connection",
        "title": "%command.testS3Connection.title%",
        "category": "Paste S3"
      },
      {
        "command": "paste-s3.undoRecentUpload",
        "title": "%command.undoRecentUpload.title%",
        "category": "Paste S3"
      },
      {
        "command": "paste-s3.clearCache",
        "title": "%command.clearCache.title%",
        "category": "Paste S3"
      }
    ],
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "pasteS3.s3.region": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.region.description%"
        },
        "pasteS3.s3.endpoint": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.endpoint.description%"
        },
        "pasteS3.s3.accessKeyId": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.accessKeyId.description%"
        },
        "pasteS3.s3.secretAccessKey": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.secretAccessKey.description%"
        },
        "pasteS3.s3.bucket": {
          "type": "string",
          "default": "",
          "description": "%configuration.s3.bucket.description%"
        },
        "pasteS3.s3.prefix": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.s3.prefix.description%"
        },
        "pasteS3.s3.publicUrlBase": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.s3.publicUrlBase.description%"
        },
        "pasteS3.s3.omitExtension": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.s3.omitExtension.description%"
        },
        "pasteS3.s3.skipExisting": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.s3.skipExisting.description%"
        },
        "pasteS3.s3.forcePathStyle": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.s3.forcePathStyle.description%"
        },
        "pasteS3.s3.clientOptions": {
          "type": "string",
          "default": "{}",
          "markdownDescription": "%configuration.s3.clientOptions.description%"
        },
        "pasteS3.workspace.path": {
          "type": "string",
          "default": "images",
          "description": "%configuration.workspace.path.description%"
        },
        "pasteS3.workspace.linkBase": {
          "type": "string",
          "default": "images/",
          "description": "%configuration.workspace.linkBase.description%"
        },
        "pasteS3.enabled": {
          "type": "boolean",
          "default": true,
          "scope": "language-overridable",
          "description": "%configuration.enabled.description%",
          "order": 0
        },
        "pasteS3.uploadDestination": {
          "type": "string",
          "default": "s3",
          "enum": [
            "s3",
            "workspace"
          ],
          "enumDescriptions": [
            "%configuration.uploadDestination.enum.s3%",
            "%configuration.uploadDestination.enum.workspace%"
          ],
          "scope": "language-overridable",
          "description": "%configuration.uploadDestination.description%",
          "order": 1
        },
        "pasteS3.fileSizeLimit": {
          "type": "integer",
          "default": 10000000,
          "scope": "language-overridable",
          "description": "%configuration.fileSizeLimit.description%"
        },
        "pasteS3.allowMultipleFiles": {
          "type": "string",
          "default": "prompt",
          "enum": [
            "prompt",
            "allow",
            "deny"
          ],
          "scope": "language-overridable",
          "description": "%configuration.allowMultipleFiles.description%"
        },
        "pasteS3.mimeTypeDetectionMethod": {
          "type": "string",
          "default": "content",
          "enum": [
            "content",
            "extension",
            "none"
          ],
          "enumItemLabels": [
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.content%",
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.extension%",
            "%configuration.mimeTypeDetectionMethod.enumItemLabel.none%"
          ],
          "markdownEnumDescriptions": [
            "%configuration.mimeTypeDetectionMethod.enum.content%",
            "%configuration.mimeTypeDetectionMethod.enum.extension%",
            "%configuration.mimeTypeDetectionMethod.enum.none%"
          ],
          "scope": "language-overridable",
          "description": "%configuration.mimeTypeDetectionMethod.description%"
        },
        "pasteS3.keepOriginalFilename": {
          "type": "boolean",
          "default": false,
          "scope": "language-overridable",
          "markdownDescription": "%configuration.keepOriginalFilename.description%"
        },
        "pasteS3.fileNamingMethod": {
          "type": "string",
          "default": "md5Short",
          "enum": [
            "md5",
            "md5Short",
            "uuid",
            "nanoid",
            "unixTimestamp",
            "readableTimestamp",
            "prompt"
          ],
          "enumItemLabels": [
            "%configuration.fileNamingMethod.enumItemLabel.md5%",
            "%configuration.fileNamingMethod.enumItemLabel.md5Short%",
            "%configuration.fileNamingMethod.enumItemLabel.uuid%",
            "%configuration.fileNamingMethod.enumItemLabel.nanoid%",
            "%configuration.fileNamingMethod.enumItemLabel.unixTimestamp%",
            "%configuration.fileNamingMethod.enumItemLabel.readableTimestamp%",
            "%configuration.fileNamingMethod.enumItemLabel.prompt%"
          ],
          "enumDescriptions": [
            "%configuration.fileNamingMethod.enum.md5%",
            "%configuration.fileNamingMethod.enum.md5Short%",
            "%configuration.fileNamingMethod.enum.uuid%",
            "%configuration.fileNamingMethod.enum.nanoid%",
            "%configuration.fileNamingMethod.enum.unixTimestamp%",
            "%configuration.fileNamingMethod.enum.readableTimestamp%",
            "%configuration.fileNamingMethod.enum.prompt%"
          ],
          "scope": "language-overridable",
          "markdownDescription": "%configuration.fileNamingMethod.description%",
          "order": 2
        },
        "pasteS3.defaultSnippet": {
          "type": "string",
          "default": "${url}",
          "scope": "language-overridable",
          "markdownDescription": "%configuration.defaultSnippet.description%",
          "order": 3
        },
        "pasteS3.imageSnippet": {
          "type": "string",
          "default": "${url}",
          "scope": "language-overridable",
          "markdownDescription": "%configuration.imageSnippet.description%",
          "order": 4
        },
        "pasteS3.undoLimit": {
          "type": "integer",
          "default": 10,
          "markdownDescription": "%configuration.undoLimit.description%"
        },
        "pasteS3.mimeTypeFilter": {
          "type": "string",
          "default": "",
          "markdownDescription": "%configuration.mimeTypeFilter.description%",
          "order": 5
        },
        "pasteS3.ignoreWorkspaceFiles": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%configuration.ignoreWorkspaceFiles.description%",
          "order": 6
        },
        "pasteS3.retrieveOriginalImage": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%configuration.retrieveOriginalImage.description%",
          "order": 7
        }
      }
    },
    "configurationDefaults": {
      "[markdown]": {
        "pasteS3.defaultSnippet": "[${1:$TM_SELECTED_TEXT}](${url})",
        "pasteS3.imageSnippet": "![${1:$TM_SELECTED_TEXT}](${url})"
      },
      "[tex][latex]": {
        "pasteS3.uploadDestination": "workspace",
        "pasteS3.fileNamingMethod": "prompt",
        "pasteS3.imageSnippet": "\\includegraphics{${url}}"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "vite build",
    "watch": "vite build --watch",
    "package": "vite build --mode production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.738.0",
    "@aws-sdk/lib-storage": "^3.738.0",
    "@changesets/cli": "^2.29.8",
    "@types/lodash": "^4.17.14",
    "@types/mime-types": "^2.1.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.19.30",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "axios": "^1.10.0",
    "eslint": "^9.16.0",
    "file-type": "^20.0.0",
    "filesize": "^10.1.6",
    "lodash": "^4.17.21",
    "md5.js": "^1.3.5",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.9",
    "typescript": "^5.7.2",
    "uuid": "^11.0.5",
    "vite": "^7.3.1"
  },
  "packageManager": "pnpm@9.15.2"
}
